      region: db.region.getAll(),
      school: db.school.getAll(),
      teachers: db.teachers,
      students: db.students,
      grades: db.grades,
      classes: db.classes
    };
    
    // ä¿å­˜åˆ°localStorage
    localStorage.setItem('msw-db', JSON.stringify(data));
    console.log('æ•°æ®åº“çŠ¶æ€å·²ä¿å­˜åˆ°localStorage');
  } catch (error) {
